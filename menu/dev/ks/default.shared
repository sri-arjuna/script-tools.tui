	script_version=0.2
#
#	Variables
#
	lbl=""
#
#	Action
#
	for after in "$4" "$5" "$6";do
		[ "-h" = "$after" ] && needHelp=true
		[ -d "$SEA_DEV_DIR/$after" ] && lbl="$after"
	done
	[ ! -z $4 ] && [ $needHelp = false ] && lbl="$4"
