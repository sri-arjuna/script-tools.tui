# sea's Project Manager default values
DEFAULT_SF_USERNAME=$USER
DEFAULT_SF_VCS=git
DEFAULT_SF_PROJECT=myProject
DEFAULT_SF_PUB_NAME=\$SF_USERNAME
DEFAULT_SF_PUB_EMAIL="< $USER @ $(hostname) >"
[ -d "$XDG_PROJECT_DIR" ] && \
	DEFAULT_SF_PATH="$XDG_PROJECT_DIR" || \
	DEFAULT_SF_PATH=$HOME/Projects
DEFAULT_SF_PATH_REMOTE=/
